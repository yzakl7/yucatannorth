{
  "hosting": {
    "public": "public",
    "origin": "*",
    "allowedHeaders": ["Content-Type", "Authorization", "Content-Length", "X-Requested-With", "Accept"],
    "methods": ["GET", "PUT", "POST", "DELETE", "OPTIONS"],
    "optionsSuccessStatus": 200,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "**",
      "function": "yucatanNorthServer"
    }]
  },
  "functions": {
    "source": ".",
    "runtime": "nodejs14",
    "ignore": [
      ".firebase/**",
      ".firebaserc",
      "firebase.json",
      "**/node_modules/**"
    ]
  }
}